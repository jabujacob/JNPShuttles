@model UserManager.Models.User

@{
    ViewBag.Title = "Details";
    if (Ajax.IsThisAjaxRequest())
    {
        Layout = null;
    }
}
<section class="content-header">
    <h1>
        @ViewBag.Title
        <small>Details</small>
    </h1>
</section>
<section class="content">
    <!-- Small boxes (Stat box) -->
    <div class="row">
        <div class="col-xs-12">

            <div class="box box-solid box-primary">
                <div class="box-header">
                    <h3 class="box-title">Detail of User</h3>
                </div><!-- /.box-header -->
                <div class="box-body table-responsive">

                    <div class="widget-sec">
                        <div class="streaming-table">
                            <table class="table table-striped table-bordered">


                                <tr>
                                    <td>Password</td>
                                    <td>@Html.DisplayFor(model => model.Password)</td>
                                </tr>
                                <tr>
                                    <td>FirstName</td>
                                    <td>@Html.DisplayFor(model => model.FirstName)</td>
                                </tr>
                                <tr>
                                    <td>MiddleName</td>
                                    <td>@Html.DisplayFor(model => model.MiddleName)</td>
                                </tr>
                                <tr>
                                    <td>LastName</td>
                                    <td>@Html.DisplayFor(model => model.LastName)</td>
                                </tr>

                                <tr>
                                    <td>Email</td>
                                    <td>@Html.DisplayFor(model => model.Email)</td>
                                </tr>

                                <tr>
                                    <td>IsActive</td>
                                    <td>@Html.DisplayFor(model => model.IsActive)</td>
                                </tr>

                                <tr>
                                    <td>Team</td>
                                    <td>@Html.DisplayFor(model => model.TeamName)</td>
                                </tr>

                                <tr>
                                    <td>Manager</td>
                                    <td>@Html.DisplayFor(model => model.Manager)</td>
                                </tr>

                                <tr>
                                    <td>Executive</td>
                                    <td>@Html.DisplayFor(model => model.Executive)</td>
                                </tr>

                                <tr>
                                    <td>Dated</td>
                                    <td>@Html.DisplayFor(model => model.Dated)</td>
                                </tr>


                            </table>
                            <div class="box-footer">
                                @Html.ActionLink("Edit", "AddOrEdit/" +Model.Id,null, new { @class = "btn btn-primary" })                                
                                @Html.ActionLink("Back to List", "Index", null, new { @class = "btn btn-primary" })
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>



